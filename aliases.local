# terminal
alias cdd="cd Desktop"
alias al="subl ~/.aliases.local"

# rails
alias rc="rails c"
alias rs="rails s"
alias lint="yarn stylelint && eslint . && rubocop && bin/rspec"
alias sf="bundle exec standardrb --fix"

# heroku
alias hc="heroku console"
alias hcs="heroku console --remote staging"
alias hcp="heroku console --remote production"
alias hdp="heroku pg:diagnose -r production"
